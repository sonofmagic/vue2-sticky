{
  "name": "@icebreakers/sticky-vue2",
  "type": "module",
  "version": "1.0.0",
  "description": "tsup(esbuild) build package template",
  "author": "ice breaker <1324318532@qq.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sonofmagic/vue2-sticky.git",
    "directory": "packages/vue-sticky-directive"
  },
  "bugs": {
    "url": "https://github.com/sonofmagic/vue2-sticky/issues"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": "./src/index.ts"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch --sourcemap",
    "build": "tsup",
    "test": "vitest run",
    "test:dev": "vitest",
    "release": "pnpm publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      }
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "devDependencies": {
    "@vitejs/plugin-vue2": "^2.3.3",
    "@vue/test-utils": "catalog:vue2",
    "vue": "catalog:vue2"
  }
}
